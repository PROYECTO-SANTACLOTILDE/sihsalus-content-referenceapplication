{
    "@openmrs/esm-login-app": {
        "provider": {
            "type": "oidc",
            "issuer": "https://${KEYCLOAK_HOST}/realms/${KEYCLOAK_REALM}",
            "clientId": "${KEYCLOAK_CLIENT_ID}",
            "redirectUri": "${SPA_URL}/",
            "scope": "openid profile email",
            "postLogoutRedirectUri": "${SPA_URL}/"
        }
    }
}